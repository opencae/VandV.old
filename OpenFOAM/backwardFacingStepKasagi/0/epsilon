FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
        offset          0;
	setAverage      off;
    }
    outlet
    {
        type            zeroGradient;
    }
    ".*Wall"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
