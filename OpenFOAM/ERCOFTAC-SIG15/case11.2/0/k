FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.754019;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
        setAverage      0;
	offset          0;
        perturb         1e-05;
        value           $internalField;
    }
    bottom
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    side1
    {
        type            slip;
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    side2
    {
        type            slip;
    }
}

// ************************************************************************* //
