FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.005;

boundaryField
{
    axis
    {
        type            symmetryPlane;
    }
    inlet
    {
        type            timeVaryingMappedFixedValue;
        setAverage      off;
	offset          0;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    outerwall
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           $internalField;
    }
    back
    {
        type            wedge;
    }
    front
    {
        type            wedge;
    }
}

// ************************************************************************* //
