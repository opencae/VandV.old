FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
        setAverage      off;
	offset          0;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    axis
    {
        type            symmetryPlane;
    }
    outerwall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    back
    {
        type            wedge;
    }

    front
    {
        type            wedge;
    }

}

// ************************************************************************* //
