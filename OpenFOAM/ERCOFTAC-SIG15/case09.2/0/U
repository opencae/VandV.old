FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (5.675893 0 0);

boundaryField
{
    inlet
    {
        type            cyclic;
    }

    outlet
    {
        type            cyclic;
    }

    empty1       
    {
        type            empty;
    }

    empty2
    {
        type            empty;
    }
 
    bottom       
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    top 
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

