FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 323.8;

boundaryField
{
    empty1
    {
        type            empty;
    }
    bottom
    {
        type            omegaWallFunction;
        value           uniform 1;
    }
    empty2
    {
        type            empty;
    }
    top
    {
        type            omegaWallFunction;
        value           uniform 1;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    } 
}

