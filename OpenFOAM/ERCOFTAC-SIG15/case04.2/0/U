FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    empty
    {
        type            empty;
    }
    inlet
    {
        type            timeVaryingMappedFixedValue;
        setAverage      off;
        perturb         1e-05;
        offset          (0 0 0);
        value           $internalField;
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
