FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    empty
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
        type            slip;
    }
    membrane
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 110.5;
    }
    wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
