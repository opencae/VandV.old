FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 459.99;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
        setAverage      0;
	offset          0;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    plate
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    cube
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    slip
    {
        type            slip;
    }

    sym
    {
        type            symmetryPlane;
    }
}

// ************************************************************************* //
