FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 110.805;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 110.805;
    }
    outlet
    {
        type            zeroGradient;
    }
    synthetic
    {
        type            fixedValue;
        value           uniform 137.13364595545134818288;
    }
    wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    front
    {
        type            empty;
    }
}

