FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
      /*
        type            parabolicVelocity;
        maxValue        2.775;
        n               (1 0 0);
        y               (0 1 0);
      */
        type            mapped;
        interpolationScheme cell;
        setAverage      true;
        average         (2.5 0 0);
        value           uniform (2.5 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    synthetic
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    front
    {
        type            empty;
    }
}

// ************************************************************************* //
