FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          setDiscreteFieldsDict;
}

Fields
(
	Uprofile
	{
		field U;          //targetField        		
                type vector;      //type "scalar" or "vector" 
