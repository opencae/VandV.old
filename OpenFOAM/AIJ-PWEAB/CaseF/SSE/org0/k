FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    x_
    {
        type            fixedValue;
        value           uniform 0;
    }
    _x
    {
        type            fixedValue;
        value           uniform 0;
    }
    y_
    {
        type            fixedValue;
        value           uniform 0;
    }
    _y
    {
        type            fixedValue;
        value           uniform 0;
    }
    z_
    {
        type            fixedValue;
        value           uniform 0;
    }
    _z
    {
        type            fixedValue;
        value           uniform 0;
    }
    targetBuilding_buildingObjects
    {
        type            fixedValue;
        value           uniform 0;
    }
}


