                  ( 0 0 140 0.2718912 )
                  ( 0 0 160 0.2302128 )
                  ( 0 0 200 0.2152227 )
                  ( 0 0 240 0.1594323 )
                  ( 0 0 280 0.1037232 )
                  ( 0 0 320 0.0855468 )
                  ( 0 0 360 0.0406272 )
                  ( 0 0 400 0.0238572 )
                  ( 0 0 470 0.0110592 )
                  ( 0 0 500 0.003 )
                );              //if type scalar (x y z p)
	}
        
	eProfile
	{
		field epsilon;
                type scalar;
		direction "z";
		internal true ;
		patchNames 
                (
                y_
