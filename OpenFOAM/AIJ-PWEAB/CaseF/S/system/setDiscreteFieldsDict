                  ( 0 0 200 0 3.85 0 )
                  ( 0 0 240 0 4.05 0 )
                  ( 0 0 280 0 4.2 0 )
                  ( 0 0 320 0 4.45 0 )
                  ( 0 0 360 0 4.6 0 )
                  ( 0 0 400 0 4.7 0 )
                  ( 0 0 470 0 4.8 0 )
                  ( 0 0 500 0 5 0 )
                );               //if type is vector (x y z Ux Uy Uz)
	}

	kProfile
	{
		field k;
                type scalar;
		direction "z";
		internal true ;
		patchNames 
                (
                y_
                );
                profile
