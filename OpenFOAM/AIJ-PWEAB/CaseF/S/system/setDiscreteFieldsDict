FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          setDiscreteFieldsDict;
}

Fields
(
	Uprofile
	{
		field U;          //targetField        		
                type vector;      //type "scalar" or "vector" 
		direction "z";    //direction "x","y", or "z" 
		internal true ;  //set internal field or not ,true or true
		patchNames
                (
                y_
                );                //patchNames to set value 
                profile
                (
                  ( 0 0 0  0 0 0 )
                  ( 0 0 10 0 1.6 0 )
                  ( 0 0 20 0 2.1 0 )
                  ( 0 0 35 0 2.3 0 )
                  ( 0 0 40 0 2.5 0 )
                  ( 0 0 50 0 2.75 0 )
                  ( 0 0 60 0 2.9 0 )
                  ( 0 0 65 0 3 0 )
