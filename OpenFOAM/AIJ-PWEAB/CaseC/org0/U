FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    x_
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    _x
    {
        type            zeroGradient;
    }

    y_
    {
        type slip;
    }

    _y
    {
        type slip;
    }

    _z
    {
        type slip;
    }

    ".*"
    {
      type fixedValue;
      value uniform (0 0 0);
    }
}

// ************************************************************************* //
