FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.052;

boundaryField
{
    x_
    {
        type            fixedValue;
	value           uniform 0;
    }

    _x
    {
        type            zeroGradient;
    }

    y_
    {
        type slip;
    }

    _y
    {
        type slip;
    }

    _z
    {
        type slip;
    }

    ".*"
    {
        type            epsilonWallFunction;
        value           uniform 0.052;
    }
}

// ************************************************************************* //
