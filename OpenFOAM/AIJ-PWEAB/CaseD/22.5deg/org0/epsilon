FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    x_
    {
        type            zeroGradient;
    }
    _x
    {
        type            zeroGradient;
    }
    y_
    {
        type            zeroGradient;
    }
    _y
    {
        type            zeroGradient;
    }
    _z
    {
        type            slip;
    }
    ".*"
    {
        type            epsilonWallFunction;
        value           uniform 1;
    }
}

// ************************************************************************* //
