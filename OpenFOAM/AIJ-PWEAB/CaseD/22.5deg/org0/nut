FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    x_
    {
        type            calculated;
        value           uniform 0;
    }
    _x
    {
        type            calculated;
        value           uniform 0;
    }
    y_
    {
        type            calculated;
        value           uniform 0;
    }
    _y
    {
        type            calculated;
        value           uniform 0;
    }
    _z
    {
        type            calculated;
        value           uniform 0;
    }
    ".*"
    {
        type            nutkWallFunction;
//        type            nutUWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
}

