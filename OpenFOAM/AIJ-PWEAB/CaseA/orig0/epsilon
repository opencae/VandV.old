FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.052;

boundaryField
{
    #include "include/slipPatches"

    x_
    {
        type            timeVaryingMappedFixedValue;
	setAverage      off;
        offset          0;
        value           $internalField;
    }

    _x
    {
        type            zeroGradient;
    }

    ".*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
}

