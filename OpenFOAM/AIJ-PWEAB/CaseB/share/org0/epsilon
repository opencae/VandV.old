FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.052;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
	setAverage      off;
        offset          0;
    }

    outlet
    {
        type            zeroGradient;
    }

    symmetry
    {
	type symmetryPlane;
    }

    lateral
    {
        type slip;
    }

    ground
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.052;
    }

    sky
    {
        type slip;
    }

    building
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.052;
    }
}

// ************************************************************************* //
