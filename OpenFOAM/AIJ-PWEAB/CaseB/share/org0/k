FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.079;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
	setAverage      off;
        offset          0;
    }

    outlet
    {
        type            zeroGradient;
    }

    symmetry
    {
	type symmetryPlane;
    }

    lateral
    {
        type slip;
    }

    ground
    {
        type            kqRWallFunction;
        value           uniform 0.079;
    }

    sky
    {
        type slip;
    }

    building
    {
        type            kqRWallFunction;
        value           uniform 0.079;
    }
}

// ************************************************************************* //
